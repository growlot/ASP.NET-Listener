<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2"
                   namespace="AMSLLC.Listener.Common.WNP.Model"
                   assembly="AMSLLC.Listener.Common.WNP">

  <class name="Comment" table="wndba.TCOMMENT" lazy="false">

    <id name="Id">
      <column name="ID" />
      <generator class="native" />
    </id>

    <many-to-one  name="Owner"
                  class="Owner"
                  column="OWNER"
                  cascade="none" />

    <property name="EquipmentNumber" column="EQP_NO" type="AnsiString" length="20" />
    <property name="EquipmentType" column="EQP_TYPE" type="AnsiString" length="2" />
    <property name="CommentIndex" column="COMMENT_INDEX" />
    <property name="CreateDate" column="CREATE_DATE" type="Timestamp"  />
    <property name="CommentText" column="COMMENTS" type="AnsiString" length="250" />
    <property name="Trouble1" column="TROUBLE1" type="AnsiString" length="3" />
  </class>

</hibernate-mapping>

