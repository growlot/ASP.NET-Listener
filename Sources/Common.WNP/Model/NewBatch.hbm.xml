<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2"
                   namespace="AMSLLC.Listener.Common.WNP.Model"
                   assembly="AMSLLC.Listener.Common.WNP">

  <class name="NewBatch" table="wndba.TNEW_BATCH" lazy="false">

    <!-- Identity mapping -->
    <id name="Number" type="AnsiString" length="10" unsaved-value="" >
      <column name="NEW_BATCH_NO" />
    </id>

    <!-- Simple mappings -->
    <property name="Status" column="NEW_BATCH_STATUS" />
    <property name="EquipmentType" column="EQP_TYPE" type="AnsiString" length="2" />
    <property name="Description" column="NEW_BATCH_DESC" type="AnsiString" length="60" />
  </class>

</hibernate-mapping>

