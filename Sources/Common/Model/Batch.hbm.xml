<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2"
                   namespace="AMSLLC.Listener.Common.Model"
                   assembly="AMSLLC.Listener.Common">

  <!-- Mappings for class 'Config' -->
  <class name="Batch" table="Batch" lazy="false">

    <!-- Identity mapping -->
    <id name="Id">
      <column name="BatchId" />
      <generator class="native" />
    </id>

    <!-- Simple mappings -->
    <property name="TotalChunks" column="TotalChunks" />
    <property name="LatestChunk" column="LatestChunk" />

  </class>

</hibernate-mapping>